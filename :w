#include "fdf.h"

int		**ft_get_map(int fd)
{
	int		i;
	int		j;
	char 	*line;
	t_line	*lst;
	t_line	*begin;
	int		**map;
	int		nb_line;

	if (!(lst = malloc(sizeof(t_line))))
		return 0;
	nb_line = 0;
	while (get_next_line(fd, &line) == 1)
	{
		ft_putendl(line);
		nb_line++;
		if (!(lst->str = ft_strsplit(line, ' ')))
			return 0;
	   	i = 0;
		while (lst->str[i])
		{
			lst->nb_col++;
			i++;
		}
		lst=lst->next;
		if (line)
			free(line);
	}
	return (begin);
}
